<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="start"
    android:minHeight="40dp"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/history_background_top"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginLeft="16dp"
            android:src="@drawable/wifi3" />

        <TextView
            android:id="@+id/type"
            style="@style/listViewHistoryRowItem"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/history_network_name"
            style="@style/listViewHistoryRowItem"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            android:textSize="16sp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/history_background_bottom"
        android:orientation="horizontal"
        android:paddingEnd="16dp"
        android:paddingStart="16dp">

        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:layout_weight="10"
            android:orientation="vertical">


            <TextView
                android:id="@+id/date"
                style="@style/listViewHistoryRowItem"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:lines="1"
                android:textSize="10sp" />


            <TextView
                android:id="@+id/device"
                style="@style/listViewHistoryRowItemGrey"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:lines="1"
                android:gravity="start"
                android:textSize="16sp" />


        </LinearLayout>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@android:color/white" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:gravity="end"
            android:orientation="vertical"
            android:padding="4dp">


            <TextView
                android:id="@+id/quality"
                style="@style/listViewHistoryRowItem"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="end" />

            <TextView
                style="@style/listViewHistoryRowItemGrey"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:text="%" />

        </LinearLayout>


        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@android:color/white" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:gravity="end"
            android:orientation="vertical"
            android:padding="4dp">


            <TextView
                android:id="@+id/ping"
                style="@style/listViewHistoryRowItem"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="end" />

            <TextView
                style="@style/listViewHistoryRowItemGrey"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:text="@string/test_ms" />
        </LinearLayout>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@android:color/white" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:gravity="end"
            android:orientation="vertical"
            android:padding="4dp">


            <TextView
                android:id="@+id/down"
                style="@style/listViewHistoryRowItem"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="end" />

            <TextView
                style="@style/listViewHistoryRowItemGrey"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:text="@string/test_mbps" />
        </LinearLayout>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@android:color/white" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:gravity="end"
            android:orientation="vertical"
            android:padding="4dp">


            <TextView
                android:id="@+id/up"
                style="@style/listViewHistoryRowItem"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="end" />

            <TextView
                style="@style/listViewHistoryRowItemGrey"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:text="@string/test_mbps" />
        </LinearLayout>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@android:color/white" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:gravity="end"
            android:orientation="vertical"
            android:padding="4dp">


            <TextView
                android:id="@+id/jitter"
                style="@style/listViewHistoryRowItem"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="end" />

            <TextView
                style="@style/listViewHistoryRowItemGrey"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:text="@string/test_ms" />
        </LinearLayout>

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@android:color/white" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:gravity="end"
            android:orientation="vertical"
            android:padding="4dp">


            <TextView
                android:id="@+id/packet_loss"
                style="@style/listViewHistoryRowItem"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="end" />

            <TextView
                style="@style/listViewHistoryRowItemGrey"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="end"
                android:text="%" />
        </LinearLayout>

    </LinearLayout>


</LinearLayout>


